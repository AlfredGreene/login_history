{
    "name": "ashabada/login_history",
    "type": "roundcube-plugin",
    "description": "Plugin for Roundcube webmail. Saves users login, date, time, ip and DNS name to MySQL upon loging in.",
    "keywords": ["login","history"],
    "homepage": "https://github.com/ashabada/login_history",
    "license": "GPL-3.0+",
    "version": "1.04",
    "authors": [
        {
            "name": "ashabada",
            "email": "staphspam@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "roundcube/plugin-installer": ">=0.1.6",
	"geoip2/geoip2": "~2.0"
    },
    "support":
    {
      "issues": "https://github.com/ashabada/login_history/issues",
      "source": "https://github.com/ashabada/login_history",
      "email": "staphspam@gmail.com"
    },
    "extra": {
        "roundcube": {
	    "sql-dir": "SQL"
        }
    }
}
